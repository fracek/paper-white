{
	"name": "Paper White",
	"type": "light",
	"colors": {
		"activityBar.background": "#160741",
		"activityBar.inactiveForeground": "#d4d7ed",
		"activityBar.foreground": "#fefeff",

		"focusBorder": "#9b45f7",
		"button.background": "#9b45f7",
		"textLink.foreground": "#9b45f7",
		"badge.background": "#9b45f7",
		"badge.foreground": "#fefeff",

		"editor.background": "#fefeff",
		"editor.foreground": "#160741",
		"editor.selectionBackground": "#e2e4f3",
		"editor.lineHighlightBackground": "#e2e4f3",

		"editorLink.activeForeground": "#59c0f8",

		"editorLineNumber.activeForeground": "#59c0f8",
		"editorLineNumber.foreground": "#000338",

		"editorGutter.addedBackground": "#8af862",
		"editorGutter.deletedBackground": "#f9718a",
		"editorGutter.modifiedBackground": "#89d3fa",

		"editorGroupHeader.tabsBackground": "#fefeff",
		"tab.activeBackground": "#fefeff",
		"tab.activeBorder": "#9b45f7",
		"tab.inactiveBackground": "#fefeff",
		"tab.border": "#fefeff",

		"gitDecoration.addedResourceForeground": "#8af862",
		"gitDecoration.deletedResourceForeground": "#f9718a",
		"gitDecoration.modifiedResourceForeground": "#89d3fa",

		"sideBar.background": "#fefeff",
		"sideBar.border": "#fefeff",
		"sideBarSectionHeader.background": "#fefeff",
		"list.activeSelectionBackground": "#e2e4f3",
		"list.activeSelectionForeground": "#000338",
		"list.focusOutline": "#fefeff",
		"list.warningForeground": "#f7c845",
		"list.errorForeground": "#f74566",

		"statusBar.background": "#160741",
		"statusBar.debuggingBackground": "#f78945",
		"statusBarItem.errorForeground": "#f9718a",
		"statusBarItem.warningForeground": "#f9d36c",

		"quickInput.background": "#fefeff",
		"quickInput.foreground": "#000338",
		"pickerGroup.foreground": "#9b45f7",

		"terminalCursor.background": "#000338",
		"terminal.background": "#fefeff",
		"terminalCursor.foreground": "#fefeff",
		"terminal.foreground": "#000338"
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#586174",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"storage.type.dev.natspec"
			],
			"settings": {
				"foreground": "#586174",
				"fontStyle": "bold italic"
			}
		},
		{
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#586174"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#000338",
				"fontStyle": "bold"
			}
		}
	]
}
