{
	"name": "Paper White",
	"type": "light",
	"colors": {
		"editor.background": "#fff",
		"editor.foreground": "#505050",
		"editor.selectionBackground": "#eee8d5",
		"editor.lineHighlightBackground": "#fdf6e3",

		"editorLink.activeForeground": "#268bd2",

		"editorLineNumber.activeForeground": "#2aa198",
		"editorLineNumber.foreground": "#505050",

		"editorGutter.addedBackground": "#859900",
		"editorGutter.deletedBackground": "#dc322f",
		"editorGutter.modifiedBackground": "#2aa198",

		"terminal.background": "#505050",
		"terminalCursor.background": "#505050",
		"terminal.foreground": "#fff",
		"terminalCursor.foreground": "#fff",
	},
	"tokenColors": [
		{
			"scope": [
				"comment",
				"string.quoted.docstring"
			],
			"settings": {
				"foreground": "#586e75",
				"fontStyle": "italic"
			}
		},
		{
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#586e75"
			}
		},
		{
			"scope": [
				"keyword",
				"storage"
			],
			"settings": {
				"foreground": "#000000",
				"fontStyle": "bold"
			}
		}
	]
}
